{
  "applications": {
    "gecko": {
      "id": "busca@ellugar.co",
      "strict_min_version": "42.0"
    }
  },
  "manifest_version": 2,
  "name": "busca.afk",
  "description": "busca.afk extension",
  "author": "Mario Carballo Zama",
  "version": "1.2.0",
  "icons": {
    "16": "img/icon.svg",
    "19": "img/icon.svg",
    "38": "img/icon.svg",
    "48": "img/icon.svg",
    "128": "img/icon.svg",
    "256": "img/icon.svg",
    "512": "img/icon.svg"
  },
  "browser_action": {
    "default_icon": "img/icon.svg",
    "default_popup": "popup.html"
  },
  "permissions": ["https://www.reddit.com/", "webRequest", "tabs", "storage"]
}
